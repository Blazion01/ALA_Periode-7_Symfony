<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <div id="navbar">
            <div>
                <h1><a href="{{path('index')}}">Dinder</a></h1>
                <nav>
                    <img width="50" height="50" src="./build/images/nav/Nav.png" alt="Nav image">
                    <ul>
                        <li>
                            <a href="#Experience">Experience</a>
                        </li>
                {% if user %}
                        <hr>
                        <li>
                            <a href="#Wag&Flag">Wag&Flag</a>
                        </li>
                        <hr>
                        <li>
                            <a href="#Flagbox">Flagbox</a>
                        </li>
                    </ul>
                </nav>
                <a id="logout" href="{{path('app_logout')}}">Log<br>Out</a>
                {% else %}
                    </ul>
                </nav>
                <a id="login" href="{{path('app_login')}}">Log<br>In</a>
                {% endif %}
            </div>
        </div>
        <div id="main">
            {% if user %}
            <p id="UserLoggedIn">Hello {{ user }}</p>
            {% endif %}
            {% block body %}{% endblock %}
        </div>
        <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.11.2/umd/popper.min.js" integrity="sha512-aDciVjp+txtxTJWsp8aRwttA0vR2sJMk/73ZT7ExuEHv7I5E6iyyobpFOlEFkq59mWW8ToYGuVZFnwhwIUisKA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    </body>
</html>
